  <article class="exercise-card">
    
        <article>
          <ul >
              <li ><p>{{ exercise.type }}</p>|</li>
              <li >
                  <img src="/assets/icons/time.svg" alt="time icon">
                  <p>{{ exercise.duration }} min</p>
              </li>
              <li >
                  <img src="/assets/icons/community-drops-color.svg" alt="community-drops icon">
                  <p>{{  countMessages }} community drops</p> 
              </li>
          </ul>
          <progress  value="10" max="100"></progress>
        </article>

        <section >
            <h1>{{ exercise.title }}</h1>
            <p>{{ exercise.description }}</p>
        </section>

          <a  href="/">Start</a>
  </article>
    
</main>
